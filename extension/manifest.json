{
    "name": "Chrome Resourse Monitor",
    "version": "1.0",
    "description": "Power Monitor extension, Icons made by Vectors Market from https://www.flaticon.com/",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "nativeMessaging",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "image/icon_gauge.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' https://www.gstatic.com; object-src 'self'"
}
