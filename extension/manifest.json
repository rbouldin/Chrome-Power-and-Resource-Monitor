{
    "name": "Power and Resource Monitor",
    "version": "1.0",
    "description": "Monitor your browser's power and resource consumption. Icon Attribution: Icon made by Vectors Market from www.flaticon.com",
    "manifest_version": 2,
    "permissions": [
        "nativeMessaging",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "image/icon_gauge.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' https://www.gstatic.com; object-src 'self'"
}
