{
    "name": "Power Monitor",
    "version": "1.0",
    "description": "Power Monitor extension",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://www.gstatic.com; object-src 'self'"
}